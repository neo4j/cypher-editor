{
  "name": "demo-base",
  "private": true,
  "description": "demo base files for cypher editor",
  "keywords": [],
  "version": "1.0.0-next.4",
  "author": "Neo4j Inc.",
  "license": "GPL-3.0",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/neo4j-contrib/cypher-editor.git"
  },
  "bugs": {
    "url": "https://github.com/neo4j-contrib/cypher-editor/issues"
  },
  "scripts": {
    "build-lib": "babel --config-file ../../babel.config.js --extensions \".js\" --out-dir lib ./src",
    "build-es": "cross-env ES=true babel --config-file ../../babel.config.js --extensions \".js\" --out-dir es ./src",
    "build": "npm run build-lib && npm run build-es",
    "clean:build": "rimraf lib && rimraf es"
  },
  "files": [
    "css/",
    "es/",
    "lib/",
    "README.md"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "neo4j-driver": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-transform-regenerator": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2"
  }
}
