{
  "name": "demo-codemirror-6-vite-svelte",
  "private": true,
  "version": "1.0.0-next.14",
  "type": "module",
  "scripts": {
    "start": "vite --port 5175",
    "deploy": "vite build",
    "serve": "pm2 serve dist 5175 --spa --name demo5175",
    "unserve": "pm2 delete demo5175",
    "serve:bg": "npx -y http-server dist --port 5175",
    "e2e": "export PORT=5175; playwright test --config=../../playwright.config.js",
    "e2e:selector": "playwright codegen http://localhost:5175",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.3.4",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/language": "^6.3.1",
    "@codemirror/lint": "^6.1.0",
    "@codemirror/search": "^6.2.3",
    "@codemirror/state": "^6.1.4",
    "@codemirror/view": "^6.6.0",
    "@lezer/highlight": "^1.1.3",
    "@neo4j-cypher/codemirror": "1.0.0-next.12",
    "@neo4j-cypher/svelte-codemirror": "1.0.0-next.14",
    "demo-base": "1.0.0-next.5",
    "demo-base-svelte": "1.0.0-next.5",
    "neo4j-driver": "^5.3.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.3.1",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.10.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
