{
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-react-display-name": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@playwright/test": "^1.25.2",
    "api-docdown": "^1.0.2",
    "cross-env": "^7.0.3",
    "pm2": "^5.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "turbo": "^1.4.7"
  },
  "workspaces": [
    "packages/*",
    "demos/*",
    "apps/*"
  ],
  "scripts": {
    "build:demos": "turbo run deploy --filter='!./apps/*'",
    "build:libs": "turbo run build",
    "build:docs": "api-docdown generate",
    "build:docs-extract": "api-docdown extract",
    "build:docs-document": "api-docdown document",
    "clean:all": "turbo run clean:deploy clean:build && yarn clean:packages",
    "clean:demos": "turbo run clean:deploy",
    "clean:packages": "rimraf packages/*/node_modules; rimraf demos/*/node_modules; rimraf apps/*/node_modules; rimraf node_modules; rimraf .yarn",
    "clean:libs": "turbo run clean:build",
    "turbo": "turbo",
    "pm2": "pm2",
    "prepublishOnly": "yarn build:libs",
    "prettier": "prettier --write .",
    "start:demos": "turbo run start",
    "start:e2e": "turbo run e2e",
    "stop:e2e": "turbo run unserve"
  },
  "name": "cypher-editor",
  "packageManager": "yarn@3.3.0"
}
